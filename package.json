{
  "name": "react-dialogista",
  "version": "0.3.0",
  "description": "Slim library for all your dialogs",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rm -rf lib",
    "lint": "eslint src/ *.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:lib": "babel src/lib --out-dir lib",
    "build:demo": "npm run clear:demo && webpack --config webpack.config.demo && cp src/demo/index.html demo/index.html",
    "clear:demo": "rm -rf demo/**",
    "preversion": "npm run clean",
    "version": "npm run build:lib",
    "postversion": "git push && git push --tags",
    "prepublish": "npm run clean && npm run build:lib"
  },
  "keywords": [
    "react",
    "react-component",
    "dialog",
    "dialogs",
    "modal",
    "popup",
    "overlay"
  ],
  "author": "Igor Ovsiannikov <garek@mail.ru>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ggarek/react-dialogista.git"
  },
  "devDependencies": {
    "babel-cli": "^6.2.0",
    "babel-core": "^6.4.5",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.2.1",
    "babel-plugin-syntax-jsx": "^6.1.18",
    "babel-plugin-transform-react-jsx": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-1": "^6.1.18",
    "eslint": "^1.10.1",
    "eslint-config-airbnb": "^1.0.0",
    "eslint-plugin-react": "^3.10.0",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "webpack": "^1.12.12"
  },
  "peerDependencies": {
    "react": "^0.14.0"
  },
  "dependencies": {
    "react-redux": "^4.0.0",
    "redux": "^3.0.4"
  }
}
